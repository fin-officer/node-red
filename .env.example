# Email configuration
EMAIL_HOST=test-smtp.example.com
EMAIL_PORT=587
EMAIL_USER=test@example.com
EMAIL_PASSWORD=test_password
EMAIL_USE_TLS=true
EMAIL_IMAP_HOST=test-imap.example.com
EMAIL_IMAP_PORT=993
EMAIL_IMAP_FOLDER=INBOX

# Ollama configuration
OLLAMA_HOST=http://ollama:11434  # Wewnętrzny port pozostaje 11434
OLLAMA_MODEL=mistral
OLLAMA_API_KEY=
OLLAMA_PORT=11435  # Zmienione z 11434 na 11435 aby uniknąć konfliktu

# SQLite configuration
SQLITE_DB_PATH=/data/sqlite/emails.db

# Node-RED configuration
NODERED_PORT=1880
NODERED_USERNAME=admin
NODERED_PASSWORD=password

# MailHog configuration
MAILHOG_SMTP_PORT=1026  # Zmienione z 1025 aby uniknąć konfliktu
MAILHOG_UI_PORT=8026    # Zmienione z 8025 aby uniknąć konfliktu

# Adminer configuration
ADMINER_PORT=8081